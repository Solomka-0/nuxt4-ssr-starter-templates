# Nuxt app env
NUXT_PUBLIC_API_URL=http://localhost:8000/api
NUXT_PUBLIC_APP_NAME=NuxtApp

# Авторизация/фичи (если требуется)
NUXT_PUBLIC_ENABLE_BETA=false

# Прочее (для nuxt.config или composables)
NUXT_PUBLIC_BASE_PATH=/
NUXT_PUBLIC_DEFAULT_LOCALE=ru

# Если нужен ключ для внешних API
NUXT_PUBLIC_MAPS_API_KEY=

# Локальные переменные только для сервера (например, токены)
API_SECRET_KEY=
